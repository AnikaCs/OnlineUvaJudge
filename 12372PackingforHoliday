#include<stdio.h>
int main()
{
	int l,w,h,t,test_case;
	scanf("%d",&t);
	for(test_case=1;test_case<=t;test_case++)
	{
		 scanf("%d%d%d",&l,&w,&h);
		 if(l<=20&&w<=20&&h<=20)
             printf("Case %d: good\n",test_case);
		else
		     printf("Case %d: bad\n",test_case); 		 
	}
	return 0;
}
